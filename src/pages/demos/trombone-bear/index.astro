---
import "./_style.css"
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, user-scalable=0, maximum-scale=1.0"
    />
    <title>trombone-bear</title>
  </head>
  <body>
    <!-- Got an input[type='range'] -->
    <div class="container">
      <select>
        <option>Design</option>
        <option>HTML</option>
        <option>CSS</option>
        <option>JavaScript</option>
      </select>
      <textarea></textarea>
      <button>Click</button>
      <input list="browsers" placeholder="Browser" />
      <datalist id="browsers">
        <option value="Edge"></option>
        <option value="Firefox"></option>
        <option value="Chrome"></option>
        <option value="Opera"></option>
        <option value="Safari"></option>
      </datalist>
      <div class="bear">
        <input type="range" min="0" max="1000" value="0" step="1" />
        <svg
          class="trombone-bear"
          xmlns="http://www.w3.org/2000/svg"
          fill="none"
          viewBox="0 0 595 352"
        >
          <path
            class="trombone-bear__torso"
            fill="#AF7128"
            stroke="#000"
            stroke-linejoin="round"
